DECLARE
	n, k, res
BEGIN
    READ n;
    READ k;
    res := 1;
    FOR i FROM 2 TO n DO
        res := res * i;
    ENDFOR
    FOR i FROM k DOWNTO 2 DO
        res := res / i;
    ENDFOR
    n := n - k;
    FOR i FROM n DOWNTO 2 DO
        res := res / i;
    ENDFOR
    WRITE res;
END